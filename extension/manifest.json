{
    "name": "PGXZ3",
    "version": "1.0",
    "description": "yo",
    "manifest_version": 2,
    "devtools_page": "devtools.html",
    "background": {
        "scripts": ["bg.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "proxy",
        "clipboardRead",
        "webRequest",
        "<all_urls>",
        "webRequestBlocking",
        "<all_urls>"
      ]
  }